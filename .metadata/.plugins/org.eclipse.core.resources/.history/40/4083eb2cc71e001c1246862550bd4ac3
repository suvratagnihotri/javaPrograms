package com.java.tutorials;

import java.io.FileNotFoundException;
import java.io.PrintStream;

public class HelloWorld {
	//Main method is static because it could be executed without creating an object of the class.
	//Program execution starts from the main method.
	//In java we can have more than one main() method.
	public static void main(String [] args) throws FileNotFoundException {
		PrintStream pto = new PrintStream("Hello");
		pto.print(true);
		System.out.print("Java with Suvrat!");
	}

}
